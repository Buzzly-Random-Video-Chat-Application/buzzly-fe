{
  "name": "buzzly",
  "version": "1.0.0",
  "description": "Buzzly is a social media platform that connects people with similar interests and passions. It allows users to create and share content, engage with others, and discover new trends and ideas.",
  "license": "MIT",
  "author": "Nguyen Quoc Thang <info.nguyenqthangwork@gmail.com>",
  "keywords": [
    "react",
    "boilerplate",
    "typescript",
    "starter",
    "vite",
    "redux",
    "material-ui",
    "rrd",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nquynqthanq/buzzly-fe"
  },
  "bugs": {
    "url": "hthttps://github.com/nquynqthanq/buzzly-fe/issues"
  },
  "homepage": "https://github.com/nquynqthanq/buzzly-fe#readme",
  "type": "module",
  "scripts": {
    "deploy": "vercel deploy --prod",
    "dev": "vite --open",
    "start": "vite --open",
    "host": "vite --open --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.3",
    "@reduxjs/toolkit": "^2.2.8",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@vitejs/plugin-react": "^4.4.1",
    "axios": "^1.7.9",
    "chart.js": "^4.4.8",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "langbase": "^1.1.29",
    "papaparse": "^5.5.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "socket.io-client": "^4.8.1",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/js-cookie": "^3.0.6",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.11.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.3.2"
  }
}
